{
	"name": "@lucidcms/plugin-resend",
	"private": true,
	"description": "The official Resend plugin for Lucid",
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"files": ["dist/**"],
	"author": "Proto Digital",
	"devDependencies": {
		"@lucidcms/config": "*",
		"@lucidcms/core": "*",
		"@types/node": "^20.2.1",
		"@biomejs/biome": "^1.6.4",
		"tsup": "^8.0.2",
		"typescript": "^5.5.2"
	},
	"peerDependencies": {
		"@lucidcms/core": "^0.10.2-alpha.0"
	},
	"dependencies": {},
	"volta": {
		"node": "22.11.0"
	},
	"engines": {
		"node": ">=22.0.0 <23.0.0"
	},
	"license": "MIT"
}

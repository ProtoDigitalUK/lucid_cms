{
	"name": "@lucidcms/plugin-s3",
	"version": "1.1.2",
	"private": false,
	"description": "The official S3 plugin for Lucid",
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"files": ["dist"],
	"author": "Proto Digital",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ProtoDigitalUK/lucid.git",
		"directory": "packages/plugin-s3"
	},
	"keywords": ["CMS", "Lucid CMS", "Lucid", "Proto Digital", "S3", "Plugin"],
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@lucidcms/config": "*",
		"@lucidcms/core": "*",
		"@types/node": "^22.15.2",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"@lucidcms/core": "^0.11.0-alpha.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.797.0",
		"@aws-sdk/s3-request-presigner": "^3.797.0"
	},
	"volta": {
		"node": "22.15.0"
	},
	"engines": {
		"node": ">=22.0.0 <23.0.0"
	},
	"license": "MIT"
}

{
    "name": "@lucidcms/admin",
    "version": "0.10.0-alpha.0",
    "description": "The admin SPA for Lucid CMS.",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:vite": "tsc --noEmit --skipLibCheck && vite build --emptyOutDir",
        "format": "biome format ./src",

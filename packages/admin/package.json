{
	"name": "@lucidcms/admin",
	"version": "0.10.2-alpha.0",
	"description": "The admin SolidJS SPA for Lucid CMS.",
	"private": false,
	"type": "module",
	"files": ["dist", "assets"],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./assets/*": "./assets/*",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsup",
		"dev": "vite",
		"build:vite": "vite build --emptyOutDir",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"author": "Proto Digital",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ProtoDigitalUK/lucid_cms.git",
		"directory": "packages/admin"
	},
	"keywords": ["CMS", "Headless", "SolidJS", "Typescript"],
	"license": "MIT",
	"devDependencies": {
		"@types/classnames": "^2.3.4",
		"@types/quill": "^2.0.14",
		"esbuild-plugin-solid": "^0.6.0",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@kobalte/core": "^0.13.7",
		"@solid-primitives/scheduled": "^1.4.4",
		"@solidjs/router": "^0.15.1",
		"@tailwindcss/vite": "^4.0.0-beta.4",
		"@tanstack/solid-query": "^5.61.5",
		"classnames": "^2.5.1",
		"fast-deep-equal": "^3.1.3",
		"i18next": "^24.0.2",
		"quill": "^2.0.2",
		"short-uuid": "^5.2.0",
		"solid-devtools": "^0.30.1",
		"solid-icons": "^1.1.0",
		"solid-js": "^1.9.3",
		"solid-toast": "^0.5.0",
		"tailwindcss": "^4.0.0-beta.4",
		"vite": "^5.3.3",
		"vite-plugin-solid": "^2.11.0"
	},
	"volta": {
		"node": "22.11.0"
	},
	"engines": {
		"node": ">=22.0.0 <23.0.0"
	}
}

        "lint": "biome lint ./src",
        "lint:fix": "biome lint --apply ./src"
    },
    "license": "MIT",
    "devDependencies": {
        "@lucidcms/core": "*",
        "@types/classnames": "^2.3.4",
        "@types/quill": "^2.0.14",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@kobalte/core": "^0.13.7",
        "@solid-primitives/scheduled": "^1.4.4",
        "@solidjs/router": "^0.15.1",
        "@tanstack/solid-query": "^5.61.5",
        "classnames": "^2.5.1",
        "fast-deep-equal": "^3.1.3",
        "i18next": "^24.0.2",
        "quill": "^2.0.2",
        "short-uuid": "^5.2.0",
        "solid-icons": "^1.1.0",
        "solid-js": "^1.9.3",
        "solid-toast": "^0.5.0",
        "vite": "^5.3.3",
        "vite-plugin-solid": "^2.11.0",
        "@tailwindcss/vite": "^4.0.0-beta.3",
        "tailwindcss": "^4.0.0-beta.3",
        "solid-devtools": "^0.30.1"
    },
    "volta": {
        "node": "20.15.0"
    }
